<?xml version="1.0"?>
<ExtensionManifest 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ExtensionBundleId="com.mycompany.freegridui" ExtensionBundleVersion="1.0.0" Version="5.0">
  <ExtensionList>
    <Extension Id="com.adobe.illustrator.freegridui.panel" Version="1.0.0"/>
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="ILST" Version="18.0"/>
    </HostList>
    <LocaleList>
      <Locale Code="All"/>
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="5.0"/>
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.adobe.illustrator.freegridui.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./html/index.html</MainPath>
          <CEFCommandLine/>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu/> <!-- menu is provided by illustrator plug-in -->
          <Geometry>
            <Size>
				<Height>226</Height>
				<Width>350</Width>
            </Size>
            <MinSize>
				<Height>226</Height>
				<Width>350</Width>
            </MinSize>
            <MaxSize>
				<Height>226</Height>
				<Width>350</Width>
            </MaxSize>
          </Geometry>
          <Icons/>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>